<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
	<diskStore path="java.io.tmpdir" />
	<defaultCache maxElementsInMemory="10000" eternal="false"
		overflowToDisk="true" timeToIdleSeconds="300" timeToLiveSeconds="180"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="120" />

	<!--
		<cache name="com.westwind.hr.db.vo.BaseSlyAlt"
		maxElementsInMemory="1000" eternal="false" timeToLiveSeconds="7200"
		timeToIdleSeconds="3600" overflowToDisk="true" />
	-->

	<cache name="org.hibernate.cache.UpdateTimestampsCache"
		maxElementsInMemory="20000" eternal="false" timeToLiveSeconds="7200"
		timeToIdleSeconds="3600" overflowToDisk="true" />
	<cache name="org.hibernate.cache.StandardQueryCache"
		maxElementsInMemory="3000" eternal="false" timeToLiveSeconds="7200"
		timeToIdleSeconds="3600" overflowToDisk="true" />

</ehcache>